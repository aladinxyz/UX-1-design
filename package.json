{
  "name": "@lskjs/modules",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev123": "nodemon --exec babel-node index.js",
    "dev": "npm run watch",
    "bootstrap": "lerna exec --parallel -- \\$LERNA_ROOT_PATH/scripts/pack-bootstrap.sh",
    "build": "lerna exec --parallel -- \\$LERNA_ROOT_PATH/scripts/pack-build.sh",
    "watch": "lerna exec --parallel -- \\$LERNA_ROOT_PATH/scripts/pack-watch.sh",
    "release": "scripts/release.sh",
    "link": "lerna exec --parallel -- \\$LERNA_ROOT_PATH/scripts/pack-link.sh",
    "bump": "lerna exec --parallel -- \\$LERNA_ROOT_PATH/scripts/pack-bump.sh",
    "dev2": "concurrently --kill-others \"yarn dev:ts\" \"yarn dev:js\"",
    "dev:js": "lerna exec --parallel -- cross-env-shell node \\$LERNA_ROOT_PATH/scripts/watch-js.js",
    "dev:ts": "lerna exec --parallel -- cross-env-shell node \\$LERNA_ROOT_PATH/scripts/watch-ts.js",
    "lint:ci": "yarn lint:js --format ./node_modules/eslint-teamcity/index.js . && yarn lint:ts **/*.ts && yarn lint:md .",
    "lint:js": "cross-env NODE_ENV=production eslint --cache --cache-location=.cache/eslint --ext .js,.jsx,.json,.html --report-unused-disable-directives",
    "lint:md": "remark -q",
    "lint:ts": "tslint -p . -c tslint.json -t stylish",
    "publish:latest": "lerna publish --exact --concurrency 1 --force-publish",
    "publish:next": "npm run publish:latest -- --npm-tag=next",
    "publish:debug": "npm run publish:latest -- --npm-tag=debug --no-push",
    "test": "node ./scripts/test.js",
    "test-latest-cra": "yarn --prefix --cwd lib/cli run test-latest-cra",
    "test:cli": "npm --prefix lib/cli run test",
    "echo": "pwd",
    "custom": "lerna exec --scope @lskjs/ui -- \\$LERNA_ROOT_PATH/scripts/pack-watch.sh",
    "custom2": "lerna exec --scope @lskjs/general -- \\$LERNA_ROOT_PATH/scripts/pack-watch.sh"
  },
  "author": "isuvorov",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged"
    }
  },
